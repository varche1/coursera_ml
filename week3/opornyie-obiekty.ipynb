    "import pandas\n",
    "from sklearn.svm import SVC\n",
    "\n",
    "data = pandas.read_csv(filepath_or_buffer='svm-data.csv', header=None)\n",
    "\n",
    "Y = data[0]\n",
    "X = data.drop(0, axis=1)\n",
    "\n",
    "clf = SVC(C=100000, kernel='linear', random_state=241)\n",
    "clf.fit(X, Y)\n",
    "\n",
    "print(clf.support_)\n",
    "print(clf.support_vectors_)"
   ]
  }
